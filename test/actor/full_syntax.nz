actor Num {
  set_num(num) {
    this.num = num;
  }

  get_num() {
    return this.num;
  }
}

actor Fraction {

  call_other(num_actor) {
    num_actor.send(
      set_num,
      this.num
    );
  }
  
  init(num, den) {
    fun smth() {
      print "hi";
    }

    this.num = num;
    this.den = den;
    smth();
  }

  get_num() {
    return this.num;
  }

  get_den() {
    return this.den;
  }

  get_this() {
    return this;
  }

  log() {
    print this.num / this.den;
  }

  divide_by(num) {
    this.den = this.den * num;
  }
}

